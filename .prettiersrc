{
  "singleQuote": true,
  "semi": false,
  "tabWidth": 2,
  "printWidth": 80,
  "arrowParens": "avoid",
  "jsxBracketSameLine": true,
  "endOfLine": "lf",
  "jsxSingleQuote": true,
  "trailingComma": "none",
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "vueIndentScriptAndStyle": false,
  "insertPragma": false,
  "disableLanguages": [],
  "overrides": [
    {
      "files": ".eslintrc.js",
      "options": {
        "parser": "json"
      }
    }
  ]
}
